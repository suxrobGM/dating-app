{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "brace-style": "off",
        "new-cap": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off"
    }
}
